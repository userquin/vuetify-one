{
  "name": "@vuetify/one",
  "version": "1.2.0",
  "private": false,
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./styles": {
      "import": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:one": "vite build --config vite.build.config.ts && yarn build:types",
    "build:dev": "vite build --mode development --config vite.build.config.ts ",
    "build:types": "rimraf types-temp && vue-tsc --pretty -p tsconfig.json && rollup --config rollup.types.config.js && rimraf types-temp",
    "watch": "yarn build:dev --watch",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.vue --fix --ignore-path .gitignore",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "yarn build:one"
  },
  "peerDependencies": {
    "@mdi/js": "6.2.95",
    "lodash-es": "^4.17.21",
    "vue": "^3.4.0",
    "vuetify": "^3.5.0"
  },
  "devDependencies": {
    "@babel/types": "^7.24.0",
    "@mdi/js": "6.2.95",
    "@rollup/plugin-alias": "^5.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.26",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "conventional-changelog-cli": "^4.1.0",
    "conventional-changelog-vuetify": "^1.2.1",
    "conventional-github-releaser": "^3.1.5",
    "core-js": "^3.36.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "rimraf": "^5.0.5",
    "roboto-fontface": "*",
    "rollup": "^4.13.0",
    "rollup-plugin-dts": "^6.1.0",
    "sass": "^1.71.1",
    "taze": "^0.13.3",
    "typescript": "^5.4.2",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.8.4",
    "vite": "^5.1.6",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^1.3.1",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.6",
    "vuetify": "3.5.9"
  }
}
